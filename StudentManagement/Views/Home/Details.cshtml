@model DeviceManagement.ViewModels.HomeDetailsViewModels
@*@model指定模型类型*@
@*@Model访问模型对象属性*@

@{
    ViewBag.Title = "设备详情页面";
}


<h3>@Model.PageTitle</h3>

<div>
    设备号：@Model.Device.Name
</div>
<div>
    设备类型：@Model.Device.ClassName
</div>
<div>
    所在城市：@Model.Device.City
</div>

@*有选择的使用js文件*@
@section Scripts{
    <script src="~/js/CustomScript.js"></script>
}


